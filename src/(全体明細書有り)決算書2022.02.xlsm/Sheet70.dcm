VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "Sheet70"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Dim i, k, c1, c2 As Integer
Dim syo As Integer
Dim cnt1, sum, sum2, sum3 As Long
Dim t, a As String
Dim myRange As Range

Private Sub CommandButton1_Click()
    
    c1 = 0
    c2 = 0
    k = 0
    syo = 0
    sum = 0
    sum2 = 0
    sum3 = 0
    t = ""
    a = ""
    
    Application.DisplayAlerts = False
    
    Set myRange = Range("I5:I300")

    cnt1 = WorksheetFunction.Count(myRange)
    
    syo = Application.RoundUp(cnt1 / 30, 0)

    k = 0
    
    'あらかじめ現在のシートを削除
    For i = 1 To 10
    
        If SheetDetect("未仕入" & "(" & i & ")") Then
                
            Worksheets("未仕入" & "(" & i & ")").Delete

        End If
    
    Next
               
    For i = 1 To syo
    
    If k = 0 Then
            
        Worksheets("未仕入").Copy After:=Worksheets("未仕入データ")
        ActiveSheet.Name = "未仕入" & "(" & i & ")"
        k = 1
        
    Else
        
        Worksheets("未仕入").Copy After:=Worksheets("未仕入" & "(" & i - 1 & ")")
        ActiveSheet.Name = "未仕入" & "(" & i & ")"
    
    End If
        
    c1 = (i - 1) * 30 + 5
    c2 = i * 30 + 4
        
        Worksheets("未仕入データ").Range(Cells(c1, 1), Cells(c2, 9)).Copy
        Worksheets("未仕入" & "(" & i & ")").Range("A5").PasteSpecial Paste:=xlValues
        
        sum = sum + Worksheets("未仕入" & "(" & i & ")").Range("G35").Value
        sum2 = sum2 + Worksheets("未仕入" & "(" & i & ")").Range("H35").Value
        sum3 = sum3 + Worksheets("未仕入" & "(" & i & ")").Range("I35").Value
        
        'タイトル№変換
'        a = Replace(Replace(Replace(Replace(Replace(Replace(Replace(Replace(Replace(Replace(i, "1", "Ⅰ"), "2", "Ⅱ"), "3", "Ⅲ"), "4", "Ⅳ"), "5", "Ⅴ"), "6", "Ⅵ"), "7", "Ⅶ"), "8", "Ⅷ"), "9", "Ⅸ"), "10", "Ⅹ")
        t = Worksheets("未仕入").Range("A1").Value & i
        
        Worksheets("未仕入" & "(" & i & ")").Range("A1").Value = t
        
        If i = syo And i <> 1 Then
        
        Worksheets("未仕入" & "(" & i & ")").Range("G36").Value = sum
        Worksheets("未仕入" & "(" & i & ")").Range("H36").Value = sum2
        Worksheets("未仕入" & "(" & i & ")").Range("I36").Value = sum3
        
        Else
        
        '総合計セルのクリア
        Worksheets("未仕入" & "(" & i & ")").Range("A36:I36").ClearFormats
        Worksheets("未仕入" & "(" & i & ")").Range("A36:I36").Clear
        Worksheets("未仕入" & "(" & i & ")").Range("A36:I36").Borders(xlEdgeTop).LineStyle = xlContinuous
             
        End If
        
    Next

End Sub

